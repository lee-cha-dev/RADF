{
  "name": "radf",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "style": "./dist/style.css",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
